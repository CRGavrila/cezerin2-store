{
  "compilerOptions": {
    "module": "commonjs",
    "noImplicitReturns": false,
    "noUnusedLocals": false,
    "sourceMap": true,
    "strict": true,
    "target": "es2017",
    "esModuleInterop": true,
    "noEmitOnError": true,
    "removeComments": true,
    "jsx": "react",
    "noImplicitAny": false
  },
  "compileOnSave": true,
  "include": ["src"]
}
