{
  "name": "cezerin2-store",
  "version": "0.36.0",
  "description": "Cezerin2 is React and Node.js based eCommerce platform. https://cezerin.org",
  "keywords": [
    "cezerin2",
    "cezerin",
    "ecommerce",
    "shopping cart",
    "shop",
    "cms"
  ],
  "homepage": "https://cezerin.org",
  "bugs": "https://github.com/cezerin2/cezerin2-store/issues",
  "license": "MIT",
  "author": {
    "name": "Restmount <jsonmount@gmail.com>",
    "url": "https://github.com/restmount"
  },
  "contributors": [
    {
      "name": "Nitin Goyal <nitingoyal.dev@gmail.com>",
      "url": "https://github.com/ngoyal16"
    },
    {
      "name": "Luke Busstra <luke.busstra@gmail.com>",
      "url": "https://github.com/lubuss"
    },
    {
      "name": "Restmount <jsonmount@gmail.com>",
      "url": "https://github.com/restmount"
    },
    {
      "name": "Sergey Onufrienko <sergey@onufrienko.com>",
      "url": "https://github.com/sonufrienko"
    }
  ],
  "main": "dist/server/index.js",
  "repository": {
    "type": "git",
    "url": "https://github.com/cezerin2/cezerin2-store"
  },
  "scripts": {
    "compile": "babel src -d dist --presets @babel/react --plugins transform-class-properties --quiet",
    "compile:watch": "concurrently -n storeWatcher,themeWatcher \"babel src -d dist --presets @babel/react --plugins transform-class-properties --watch\" \"babel theme/src -d theme/dist --presets @babel/react --plugins transform-class-properties --watch\"",
    "webpack:store": "webpack --config webpack.config.store.js --mode production",
    "webpack:store:watch": "webpack --config webpack.config.store.js --mode development --watch",
    "theme:install": "./scripts/theme-install.sh",
    "theme:export": "./scripts/theme-export.sh",
    "theme:build": "babel theme/src -d theme/dist --presets @babel/react --plugins transform-class-properties --quiet && npm run webpack:store",
    "build": "npm run compile && npm run theme:build",
    "build:watch": "concurrently -n compile,webpackStore \"npm run compile:watch\" \"npm run webpack:store:watch\"",
    "lint": "eslint .",
    "lint:fix": "eslint . --fix",
    "prettier": "prettier --write \"src/**/*.js\" \"theme/**/*.js\"",
    "precommit": "lint-staged",
    "start": "node -r esm dist/server/index.js"
  },
  "lint-staged": {
    "*.js": [
      "prettier --write",
      "git add"
    ]
  },
  "dependencies": {
    "@babel/cli": "^7.8.4",
    "@babel/core": "^7.8.7",
    "babel-eslint": "^10.1.0",
    "babel-loader": "^8.0.6",
    "babel-plugin-transform-class-properties": "^6.24.1",
    "@babel/polyfill": "^7.8.7",
    "@babel/preset-env": "^7.8.7",
    "@babel/preset-react": "^7.8.3",
    "body-parser": "^1.19.0",
    "cezerin2-client": "^0.36.0",
    "clean-webpack-plugin": "^3.0.0",
    "concurrently": "^5.1.0",
    "cookie-parser": "^1.4.5",
    "css-loader": "^3.4.2",
    "cssnano": "^4.1.10",
    "eslint": "^6.8.0",
    "eslint-config-airbnb": "^18.1.0",
    "eslint-config-prettier": "^6.10.0",
    "eslint-plugin-import": "^2.20.1",
    "eslint-plugin-jsx-a11y": "^6.2.3",
    "eslint-plugin-prettier": "^3.1.2",
    "eslint-plugin-react": "^7.19.0",
    "esm": "^3.2.25",
    "express": "^4.17.1",
    "express-jwt": "^5.3.1",
    "fs-extra": "^8.1.0",
    "handlebars": "^4.7.3",
    "helmet": "^3.21.3",
    "html-webpack-plugin": "^3.2.0",
    "jsonwebtoken": "^8.5.1",
    "lint-staged": "^10.0.8",
    "lodash": "^4.17.15",
    "lru-cache": "^5.1.1",
    "lscache": "^1.3.0",
    "mini-css-extract-plugin": "^0.9.0",
    "moment": "^2.24.0",
    "node-fetch": "^2.6.0",
    "node-sass": "^4.13.1",
    "postcss-loader": "^3.0.0",
    "prettier": "^1.19.1",
    "prop-types": "^15.7.2",
    "query-string": "^6.11.1",
    "react": "^16.13.0",
    "react-chartjs-2": "^2.9.0",
    "react-dom": "^16.13.0",
    "react-dropzone": "^10.2.1",
    "react-helmet": "^5.2.1",
    "react-redux": "^7.2.0",
    "react-router": "^5.1.2",
    "react-router-dom": "^5.1.2",
    "react-scroll": "^1.7.16",
    "react-sortable-hoc": "^1.11.0",
    "react-stripe-elements": "^6.1.0",
    "react-tagsinput": "^3.19.0",
    "redux": "^4.0.5",
    "redux-form": "^8.3.1",
    "redux-form-material-ui": "^4.3.4",
    "redux-thunk": "^2.3.0",
    "response-time": "^2.3.2",
    "sass-loader": "^8.0.2",
    "sitemap": "^6.1.0",
    "slug": "^2.1.1",
    "stripe": "^8.32.0",
    "theme": "file:theme",
    "typescript": "^3.8.3",
    "ua-parser-js": "^0.7.21",
    "webpack": "^4.42.0",
    "webpack-cli": "^3.3.11",
    "winston": "^3.2.1",
    "workbox-webpack-plugin": "^4.3.1"
  }
}
